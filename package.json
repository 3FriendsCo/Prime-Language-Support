{
  "name": "Prime",
  "displayName": "Prime Extension",
  "description": "Prime Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
